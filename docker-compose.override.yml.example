# Example override file for custom port mappings
# Copy this to docker-compose.override.yml if you need different ports
# (e.g., if you already have NATS running on the default ports)

services:
  wolverine-nats-test:
    ports:
      - "4223:4222"  # Client connections on 4223 instead of 4222
      - "8223:8222"  # HTTP monitoring on 8223 instead of 8222
      - "6223:6222"  # Cluster routing on 6223 instead of 6222